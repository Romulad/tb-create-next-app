{
  "name": "tb-create-next-app",
  "version": "1.1.0",
  "description": "Custom cli tool to create a Nextjs app with Ts and App Router",
  "scripts": {
    "dev": "shx rm -rf ./dist; ncc build ./src/index.ts -w -o ./dist",
    "test:dev": "vitest dev",
    "test": "vitest run",
    "preview": "shx rm -rf ./dist; ncc build ./src/index.ts -o ./dist && node ./dist/index.js",
    "build": "shx rm -rf ./dist; ncc build ./src/index.ts -o ./dist --minify --no-cache --no-source-map-register",
    "prerelease": "shx rm -rf ./dist/src && shx mkdir -p ./dist/src && shx cp -r ./src/templates ./dist/src",
    "release": "npm publish"
  },
  "keywords": [
    "react",
    "nextjs",
    "cli"
  ],
  "bin": {
    "tb-create-next-app": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "author": "Romuald Oluwatobi <romualdnoualinon@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-typescript": "^7.26.0",
    "@inquirer/prompts": "^7.3.2",
    "@types/node": "^22.13.4",
    "@types/validate-npm-package-name": "^4.0.2",
    "@vercel/ncc": "^0.38.3",
    "@vitest/coverage-v8": "^3.0.9",
    "cli-testing-library": "^3.0.1",
    "commander": "^13.1.0",
    "conf": "^13.1.0",
    "fast-glob": "^3.3.3",
    "picocolors": "^1.1.1",
    "shx": "^0.4.0",
    "typescript": "^5.7.3",
    "validate-npm-package-name": "^6.0.0",
    "vitest": "^3.0.9"
  }
}
